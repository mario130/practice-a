- var pageTitle = "Homepage";
<!DOCTYPE html>
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    title #{pageTitle}
    include pug/layout/styles
  body
    .landing-page
      .overlay
      .navbar
        .container
          .navbar-elements
            .logo twenty
              span 11
            ul.nav-links
              li
                a(href="#") Home
              li
                a(href="#") Our Rent
              li
                a(href="#") Why US
              li
                a(href="#") FAQ
              li
                a(href="#") Apply Now
            .burger
              .line1
              .line2
              .line3
      .get-started
        h2 Charitable housing company
        p Lorem ipsum dolor sit amet, consectetur adipisicing elit. Iusto inventore iure esse minus, quam expedita.
        button Get started!

    //- wave
    svg(xmlns='http://www.w3.org/2000/svg' viewbox='0 0 1440 320')
